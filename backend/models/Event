const mongoose = require('mongoose')

const Schema = mongoose.Schema

const eventSchema = new Schema({
    googleId: {
        type: String,
        required: true
    }, 
    title: {
        type: String,
        required: true
    },
    description: {
        type: String
    },
    start: {
        type: Date,
        required: true
    },
    end: {
        type: Date,
        required: true
    },
    userId: {
        type: Number,
        required: true
    }

}, { timestamps: true })

module.exports = mongoose.model('Event', eventSchema)